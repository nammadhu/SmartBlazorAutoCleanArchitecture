<Router AppAssembly="typeof(BlazorAuto.Shared.Services.IFormFactor).Assembly">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(BlazorAuto.Shared.Layout.MainLayout)">
            <NotAuthorized>
                <RedirectToLogin /> 
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>
